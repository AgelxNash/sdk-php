{
    "name": "denvolj/pact-php",
    "type": "library",
    "description": "PHP library for pact.im",
    "keywords": ["library", "pact"],
    "homepage": "https://pact.im",
    "license": "MIT",
    "authors": [
        {
            "name": "DenVolzhankin & contributors",
            "homepage": "https://github.com/denvolj/php-pact"
        }
    ],
    "require": {
      "php": ">=5.6.0",
      "ext-curl": "*",
      "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Pact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pact\\": "tests/"
        }
    }
}